class RecentCounter:

    def __init__(self):
        self.counter = []

    def ping(self, t: int) -> int:
        self.counter.append(t)
        start = 0
        count = 0
        for i in range (len(self.counter)):
            if t - self.counter[i] > 3000:
                start = i
            else:
                count += 1
        self.counter = self.counter[start:]
        return count
# 9, 43


# Your RecentCounter object will be instantiated and called as such:
# obj = RecentCounter()
# param_1 = obj.ping(t)
