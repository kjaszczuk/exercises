class RecentCounter:

    def __init__(self):
        self.counter = []

    def ping(self, t: int) -> int:
        self.counter.append(t)
        count = 0
        for i in reversed(self.counter):
            if t - i > 3000:
                return count
            else:
                count += 1
        return count
# 12, 9


# Your RecentCounter object will be instantiated and called as such:
# obj = RecentCounter()
# param_1 = obj.ping(t)
